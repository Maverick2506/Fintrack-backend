{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "crons": [
    {
      "path": "/api/daily-check",
      "schedule": "0 0 * * *"
    },
    {
      "path": "/api/recurring/trigger-recurring",
      "schedule": "0 1 1 * *"
    }
  ]
}
